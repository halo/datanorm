#!/usr/bin/env ruby
# frozen_string_literal: true

require 'bundler/setup'
require 'datanorm'

ENV['DEBUG_DATANORM'] = 'true'

# path = '/Users/orange/Desktop/datanorm.001'
path = '/Users/orange/Desktop/A6V13530363_de/V4/DATANORM.300'
document = Datanorm::Document.new(path:)

puts document.header

document.items do |item, iteration, total_iterations|
  puts "#{iteration}/#{total_iterations} #{item}"

  exit if iteration > 1000
end
